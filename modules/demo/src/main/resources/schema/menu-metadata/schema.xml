<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<metadata>
  <display-name>Menu</display-name>
  <form>
    <input name="menuItem" type="Checkbox">
      <label>Menu Item</label>
      <immutable>false</immutable>
      <indexed>true</indexed>
      <custom-text/>
      <occurrences minimum="0" maximum="1"/>
      <help-text>Check this to include this Page in the menu</help-text>
    </input>
    <input name="menuName" type="TextLine">
      <label>Menu name</label>
      <immutable>false</immutable>
      <indexed>true</indexed>
      <custom-text/>
      <occurrences minimum="0" maximum="1"/>
      <help-text>Name to be used in menu</help-text>
    </input>
  </form>
</metadata>
