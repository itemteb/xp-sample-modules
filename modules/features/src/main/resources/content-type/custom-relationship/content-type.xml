<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<content-type>
  <!-- This content type references custom relationship-types. -->
  <display-name>Custom Relationship</display-name>
  <super-type>system:structured</super-type>
  <is-abstract>false</is-abstract>
  <is-final>true</is-final>
  <is-built-in>false</is-built-in>
  <allow-child-content>true</allow-child-content>
  <form>
    <input name="cited-in" type="Relationship">
      <label>Cited In</label>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="0"/>
      <config>
        <relationship-type>com.enonic.wem.modules.features:citation</relationship-type>
      </config>
    </input>
    <input name="media-in-use" type="Relationship">
      <label>Media in use</label>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <occurrences minimum="1" maximum="1"/>
      <config>
        <relationship-type>com.enonic.wem.modules.features:media-content</relationship-type>
      </config>
    </input>
  </form>
</content-type>
