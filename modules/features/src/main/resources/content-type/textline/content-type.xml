<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<content-type>
  <display-name>Text Line</display-name>
  <super-type>base:structured</super-type>
  <is-abstract>false</is-abstract>
  <is-final>true</is-final>
  <is-built-in>false</is-built-in>
  <allow-child-content>true</allow-child-content>
  <form>
    <input name="textline" type="TextLine">
      <label>Text Line</label>
      <immutable>false</immutable>
      <indexed>true</indexed>
      <custom-text>Custom text</custom-text>
      <occurrences minimum="0" maximum="1"/>
      <help-text>Some help text</help-text>
    </input>
    <input name="requiredTextLine" type="TextLine">
      <label>Required Text Line</label>
      <immutable>false</immutable>
      <indexed>true</indexed>
      <custom-text>Custom text</custom-text>
      <occurrences minimum="1" maximum="0"/>
      <help-text>Some help text</help-text>
    </input>
  </form>
</content-type>
