<?xml version="1.0" encoding="UTF-8"?>
<content-type>
  <display-name>Medlem</display-name>
  <content-display-name-script>$('name')</content-display-name-script>
  <super-type>base:structured</super-type>
  <form>
    <input type="TextLine" name="name">
      <label>Navn</label>
      <indexed>true</indexed>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <mixin-reference name="address">
      <reference>com.enonic.wem.modules.seedsavers:address</reference>
    </mixin-reference>

  </form>
</content-type>
